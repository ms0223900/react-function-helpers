  "bugs": {
    "url": "https://github.com/ms0223900/react-function-helpers/issues"
  },
  "homepage": "https://github.com/ms0223900/react-function-helpers#readme",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  }
